extends layout

block content
  h1= title
  - let image_names = ['roc1.jpg','roc2.jpg','roc3.jpg','roc4.jpg','roc5.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]
      img(src=`images/${iname}` alt='rocket' style='width:300px; height:300px;')
  - }